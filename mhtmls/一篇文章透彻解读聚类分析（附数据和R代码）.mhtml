library(cluster)
library(ggplot2)

## DATA PREPARATION ##
# Load data
product &lt;- read.csv("*/M2_IFI_Data_Product.csv", header =3D TRUE, sep =
=3D ",", dec =3D ".")
attach(product)
View(product)

# Delete variable ID
product &lt;- product[,-1]

# Modifiy Children variable type
product$Children &lt;- as.factor(product$Children)

# Compute distance matrix
dmatrix &lt;- daisy(product)
summary(dmatrix)

## K-MEANS CLUSTERING ##

# K-means clustering for k in [4,8]
km4 &lt;- kmeans(dmatrix, 4)
km5 &lt;- kmeans(dmatrix, 5)
km6 &lt;- kmeans(dmatrix, 6)
km7 &lt;- kmeans(dmatrix, 7)
km8 &lt;- kmeans(dmatrix, 8)

# Add column with cluster number in the product data frame
product &lt;- data.frame(product, km4$cluster)
product &lt;- data.frame(product, km5$cluster)
product &lt;- data.frame(product, km6$cluster)
product &lt;- data.frame(product, km7$cluster)
product &lt;- data.frame(product, km8$cluster)
attach(product)

# Distribution of classes (Product=3DOui/Non) by cluster
table(km4.cluster, Product)
table(km5.cluster, Product)
table(km6.cluster, Product)
table(km7.cluster, Product)
table(km8.cluster, Product)

# Histograms of cluster sizes with class in color
qplot(km4$cluster, data=3Dproduct, fill=3DProduct)
qplot(km5$cluster, data=3Dproduct, fill=3DProduct)
qplot(km6$cluster, data=3Dproduct, fill=3DProduct)
qplot(km7$cluster, data=3Dproduct, fill=3DProduct)
qplot(km8$cluster, data=3Dproduct, fill=3DProduct)

#??? Display distribution of classes for 4-means clusters vs. Chlidren and =
Family_Quotient
qplot(Children, km4$cluster, data=3Dproduct, color=3DProduct) + geom_jitter=
(width =3D 0.3, height =3D 0.3)
qplot(Family_Quotient, km4$cluster, data=3Dproduct, color=3DProduct) + geom=
_jitter(width =3D 0, height =3D 0.3)

## HIERARCHICAL CLUSTERING ##

# Run the hclust algorithm
hc &lt;- hclust(dmatrix, method=3D"ward.D2")

# Display the dendrogram
plot(hc)

# Generate clusters for different k values
ghc4 &lt;- cutree(hc, k=3D4)
ghc5 &lt;- cutree(hc, k=3D5)
ghc6 &lt;- cutree(hc, k=3D6)
ghc7 &lt;- cutree(hc, k=3D7)
ghc8 &lt;- cutree(hc, k=3D8)

# Distribution of classes (Product=3DOui/Non) by cluster
product &lt;- data.frame(product, ghc4)
product &lt;- data.frame(product, ghc5)
product &lt;- data.frame(product, ghc6)
product &lt;- data.frame(product, ghc7)
product &lt;- data.frame(product, ghc8)
table(product$ghc4, Product)
table(product$ghc5, Product)
table(product$ghc6, Product)
table(product$ghc7, Product)
table(product$ghc8, Product)

# Display clusters for k in [4,8]
rect.hclust(hc, k=3D4, border=3D"red")
rect.hclust(hc, k=3D5, border=3D"blue")
rect.hclust(hc, k=3D6, border=3D"green")
rect.hclust(hc, k=3D7, border=3D"gold")
rect.hclust(hc, k=3D8, border=3D"skyblue")

# Histograms of cluster sizes with class in color
qplot(product$ghc4, data=3Dproduct, fill=3DProduct)
qplot(product$ghc5, data=3Dproduct, fill=3DProduct)
qplot(product$ghc6, data=3Dproduct, fill=3DProduct)
qplot(product$ghc7, data=3Dproduct, fill=3DProduct)
qplot(product$ghc8, data=3Dproduct, fill=3DProduct)

## FOR LOOPS #

# Computing distance matrix
dmatrix &lt;- daisy(product)

# K-means executions for a range of k values
for (i in 4:8){
  km &lt;- kmeans(dmatrix, i)
  product &lt;- data.frame(product, km$cluster)
  print(table(km$cluster, Product))
  print(qplot(km$cluster, data=3Dproduct, fill=3DProduct))
} =20
